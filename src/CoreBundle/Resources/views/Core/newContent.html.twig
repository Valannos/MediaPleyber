{# empty Twig template #}
{% extends "base.html.twig" %}
{% block stylesheet %}

    <link rel="stylesheet" href="{{ asset('css/newContent.css') }}" />
{% endblock %}
{% block header %}

    {{ parent() }}


{% endblock %}

{% block nav %}

    {% include 'CoreBundle:Core:navbar.html.twig' %}


{% endblock %}

{% block section %}
    <section>

        {% for media in newContent %}
            <article>
                <div>
                    <h4>{{media.title}}</h4>
                    <img scr="" alt="{{media.title}}">
                </div>
                <div>
                    {% if media.statut == 1 %}
                        <p class="available">Disponible</p>
                    {% endif %}

                    {% if media.statut == 2 %}
                        <p class="reserved">Réservé</p>
                    {% endif %}

                    {% if media.statut == 3 %}
                        <p class="available">Empreuté</p>
                    {% endif %}
                </div>

            </article>

        {% endfor %}



    </section>


{% endblock %}